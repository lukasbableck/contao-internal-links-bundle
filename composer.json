{
    "name": "lukasbableck/contao-internal-links-bundle",
    "type": "contao-bundle",
	"description": "This bundle automatically links pages, news, events and FAQs by their set keywords",
	"keywords": [
		"internal",
		"links",
		"linkjuice",
		"onpage",
		"seo"
	],
    "license": "LGPL-3.0-or-later",
	"authors": [
		{
			"name": "Lukas Bableck",
			"email": "lukas@bableck.dev",
			"homepage": "https://bableck.dev"
		}
	],
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^4.13 || ^5.3"
    },
	"suggest": {
		"contao/calendar-bundle": "^4.13 || ^5.3",
		"contao/faq-bundle": "^4.13 || ^5.3",
		"contao/news-bundle": "^4.13 || ^5.3"
	},
	"conflict": {
		"contao/manager-plugin": "<2.0 || >=3.0"
	},
	"require-dev": {
		"contao/calendar-bundle": "^4.13 || ^5.3",
		"contao/faq-bundle": "^4.13 || ^5.3",
		"contao/manager-plugin": "^2.0",
		"contao/news-bundle": "^4.13 || ^5.3",
		"friendsofphp/php-cs-fixer": "^3.57"
	},
    "autoload": {
        "psr-4": {
            "Lukasbableck\\ContaoInternalLinksBundle\\": "src/"
        }
    },
	"extra": {
		"contao-manager-plugin": "Lukasbableck\\ContaoInternalLinksBundle\\ContaoManager\\Plugin"
	}
}
